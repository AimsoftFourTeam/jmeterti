            <stringProp name="script">import com.eclipsesource.json.Json;
import com.eclipsesource.json.JsonObject;

String res = prev.getResponseDataAsString();

boolean isError = false;

		StringBuilder errorDetails = new StringBuilder();

JsonObject json = Json.parse(res).asObject();
String id = json.get(&quot;id&quot;).asString();
String kind = json.get(&quot;kind&quot;).asString();
log.info(&quot;id:&quot; + id + &quot;,kind:&quot; + kind);
	
		if (!&quot;001&quot;.equals(id)) {
		    errorDetails.append(&quot;idが想定値[&quot;);
		    errorDetails.append(id);
		    errorDetails.append(&quot;]ではありませんでした。\n\r&quot;);
		    isError = true;
		}
		if (!&quot;A001&quot;.equals(kind)) {

		    errorDetails.append(&quot;kindが想定値[&quot;);
		    errorDetails.append(kind);
		    errorDetails.append(&quot;]ではありませんでした。\n\r&quot;);
		    isError = true;
		}


if (isError) {
	
  AssertionResult.setFailureMessage(&quot;エラー詳細 \n\r&quot; + errorDetails.toString());
  AssertionResult.setFailure(true);
}</stringProp>
